{
    "name": "tacocat-gallery-sveltekit",
    "version": "0.0.4",
    "private": true,
    "description": "Javascript front end for the tacocat.com photo gallery, built with Sveltekit",
    "keywords": [
        "javascript",
        "typescript",
        "svelte",
        "sveltekit"
    ],
    "license": "MIT",
    "author": "Dean Moses",
    "repository": {
        "type": "git",
        "url": "git://github.com/deanmoses/tacocat-gallery-sveltekit"
    },
    "scripts": {
        "dev": "vite dev",
        "build-debug": "vite build --mode development --sourcemap true --minify false",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "deploy-staging": "./deploy_staging.sh",
        "deploy-prod": "./deploy_prod.sh"
    },
    "devDependencies": {
        "@playwright/test": "1.49.1",
        "@sveltejs/adapter-static": "^3.0.1",
        "@sveltejs/kit": "^2.0.6",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "@zerodevx/svelte-toast": "^0.9.6",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-svelte": "^2.35.1",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.2",
        "svelte": "^4.2.8",
        "svelte-check": "^3.6.2",
        "svelte-eslint-parser": "^0.43.0",
        "svelte-preprocess": "^5.1.3",
        "tslib": "^2.8.1",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.19.0",
        "vite": "^5.0.10"
    },
    "dependencies": {
        "idb-keyval": "^6.2.1",
        "immer": "^10.1.1",
        "quill": "^2.0.3",
        "svelte-easy-crop": "^3.0.1"
    },
    "engineStrict": true,
    "engines": {
        "node": ">=18.0.0"
    },
    "type": "module"
}
